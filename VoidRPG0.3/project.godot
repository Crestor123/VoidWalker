; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Character",
"language": "GDScript",
"path": "res://Scripts/Resources/Character.gd"
}, {
"base": "Resource",
"class": "Enemy",
"language": "GDScript",
"path": "res://Scripts/Resources/Enemy.gd"
}, {
"base": "Node2D",
"class": "Game",
"language": "GDScript",
"path": "res://Scripts/Map.gd"
}, {
"base": "Resource",
"class": "Island",
"language": "GDScript",
"path": "res://Scripts/Resources/Island.gd"
}, {
"base": "Resource",
"class": "Skill",
"language": "GDScript",
"path": "res://Scripts/Resources/Skill.gd"
} ]
_global_script_class_icons={
"Character": "",
"Enemy": "",
"Game": "",
"Island": "",
"Skill": ""
}

[application]

config/name="VoidRPG"
run/main_scene="res://Scenes/Game.tscn"
config/icon="res://icon.png"

[display]

window/size/width=270
window/size/height=480
window/size/test_width=540
window/size/test_height=960
window/handheld/orientation="portrait"
window/ios/hide_home_indicator=false
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"

[global]

display=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://default_env.tres"
